<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="MaterialDrawerTheme.Light" >
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/wema</item>
        <item name="colorPrimaryDark">@color/md_black_1000</item>
        <item name="colorAccent">@color/wema</item>
        <item name="android:navigationBarColor">@color/wema_transparent</item>
        <item name="android:windowContentTransitions" tools:targetApi="lollipop">true</item>


        <!-- MaterialDrawer specific values -->
        <item name="material_drawer_selected">@color/wema</item>
        <item name="material_drawer_selected_text">@color/md_black_1000</item>
    </style>



</resources>